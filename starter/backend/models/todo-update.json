{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "todo",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 2
        },
        "dueDate": {
            "type": "string"
        },
        "done": {
            "type": "boolean"
        }

    },
    "required": ["name", "dueDate", "done"],
    "additionalProperties": false
}